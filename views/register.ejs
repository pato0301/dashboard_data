<%- include('partials/head.ejs') -%>

<%- include('partials/navbarLogin.ejs',{page:page}) -%>
<div class="text-center container body-wrapper">
    
    <form class="form-signin" method="POST">
        <img class="mb-4" src="/logo/svg-logo.png" alt="" width="72" height="72">
        <h1 class="h3 mb-3 font-weight-normal">Register new user</h1>
        <label for="inputEmail" class="sr-only">Email address</label>
        <input type="email" id="inputEmail" name="email" class="form-control" placeholder="Email address" required autofocus>
        <label for="inputPassword" class="sr-only">Password</label>
        <input type="password" id="inputPassword" name="password" class="form-control" placeholder="Password" required>
        <!-- <div class="checkbox mb-3">
            <label>
                <input type="checkbox" value="remember-me"> Remember me
            </label>
        </div> -->
        <button class="btn btn-lg btn-primary btn-block mt-5" type="submit">Register</button>
        <!-- <p class="mt-5 mb-3 text-muted">&copy; 2017-2020</p> -->
    </form>
</div>

<%- include('partials/footer.ejs') -%>